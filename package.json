{
	"name": "kth-iprog-project",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/jest": "24.0.11",
		"@types/node": "11.11.3",
		"@types/react": "16.8.8",
		"@types/react-bootstrap-typeahead": "^3.4.4",
		"@types/react-dom": "16.8.2",
		"@types/react-router-dom": "^4.3.1",
		"bootstrap": "^4.3.1",
		"formik": "^1.5.2",
		"query-string": "^6.4.2",
		"react": "^16.8.4",
		"react-bootstrap-typeahead": "^4.0.0-alpha.9",
		"react-dom": "^16.8.4",
		"react-facebook-login": "^4.1.1",
		"react-pure-lifecycle": "^3.0.0",
		"react-router-dom": "^4.4.0",
		"react-scripts": "2.1.8",
		"typescript": "3.3.3333",
		"yarn": "^1.13.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "yarn ts-lint && yarn sass-lint",
		"lint:fix": "yarn ts-lint:fix && yarn sass-lint:fix",
		"ts-lint": "tslint --project tsconfig.json \"src/**/*.{ts,tsx}\"",
		"ts-lint:fix": "tslint --project tsconfig.json --fix \"src/**/*.{ts,tsx}\"",
		"sass-lint": "sass-lint -c .sass-lint.yml \"src/**/*.scss\" -v -q",
		"sass-lint:fix": "sass-lint-auto-fix -c .sass-lint.yml"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@types/react-facebook-login": "^4.1.0",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"node-sass": "^4.11.0",
		"prettier": "^1.16.4",
		"sass-lint": "^1.12.1",
		"sass-lint-auto-fix": "^0.16.1",
		"tslint": "^5.14.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^8.0.1",
		"tslint-react": "^3.6.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"yarn ts-lint",
			"git add"
		],
		"*.{sass,scss}": [
			"prettier --write",
			"yarn sass-lint",
			"git add"
		],
		"*.{js,jsx,json,css,md}": [
			"prettier --write",
			"git add"
		]
	}
}
